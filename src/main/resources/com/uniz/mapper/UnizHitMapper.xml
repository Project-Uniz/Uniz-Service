<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.uniz.mapper.UnizHitMapper">

	<select id="read" resultType = "com.uniz.domain.VideoDataVO">
	<![CDATA[
	
	select* from videodata where videoSN = #{vodeoSN}
	
	]]>
	</select>	
	
	<select id="getHitList" resultType = "com.uniz.domain.VideoDataVO">
	<![CDATA[
	
	SELECT *
	
	FROM videoData
	
	order by viewCnt+likeCnt desc
		
		
	]]>
	</select>	
	
	
	<select id="getVideo" resultType="com.uniz.domain.VideoDataVO">
		SELECT * FROM videoData where videoSn = #{videoSN}
	</select>
	
</mapper>